frame = input_0.get()\n                \n                if frame is not None:\n\n                    if blur_type == 'Gaussian':\n                        blurred_img = cv.GaussianBlur(frame, kernel, 0)\n                        \n                    elif blur_type == 'Averaging':\n                        blurred_img = cv.medianBlur(frame, kernel[0])\n                        \n                    elif blur_type == 'Median':\n                        blurred_img = cv.blur(frame, kernel)\n                        \n                    output_0.add(blurred_img)\n                    control_data[0] += 1\n\n            sleep(control_data[1])\n            \n    except KeyboardInterrupt: \n    \n        input_0.release()\n        enable_wire.release()\n        output_0.release()\n","params":[{"name":"Frequency"},{"name":"BlurType"},{"name":"Kernel"}],"ports":{"in":[{"name":"Input"},{"name":"Enable"}],"out":[{"name":"Output"}]},"size":{"width":"708px","height":"319px"}}},"d7f13bbe-df89-444d-845d-540c75dcc86e":{"id":"d7f13bbe-df89-444d-845d-540c75dcc86e","type":"basic.input","selected":false,"x":59,"y":286,"ports":[{"id":"23975c88-48de-4108-be98-f8164f6c92ac","type":"port.output","x":145,"y":305.1000061035156,"name":"input-out","alignment":"right","parentNode":"d7f13bbe-df89-444d-845d-540c75dcc86e","links":["a773172c-9529-46b3-9fe2-c5c248b731c5"],"in":false,"label":"input-out"}],"data":{"name":"Image"}},"2e65e557-46fb-41e7-92f1-58b5e3db62d4":{"id":"2e65e557-46fb-41e7-92f1-58b5e3db62d4","type":"basic.input","selected":false,"x":50,"y":431,"ports":[{"id":"60a545d0-d94d-4dd5-b53a-9f193e08043c","type":"port.output","x":136,"y":450.10003662109375,"name":"input-out","alignment":"right","parentNode":"2e65e557-46fb-41e7-92f1-58b5e3db62d4","links":["f135512d-50eb-4c8a-92af-c9b6d861940d"],"in":false,"label":"input-out"}],"data":{"name":"Enable"}},"c77ad4f1-650e-4a6c-a629-0006ffdbd4c7":{"id":"c77ad4f1-650e-4a6c-a629-0006ffdbd4c7","type":"basic.output","selected":false,"x":1064,"y":374,"po